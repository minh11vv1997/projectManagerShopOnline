@model List<SharedObject.ValueObjects.VBillDetail>
@for (int i = 0; i < Model.Count; i++)
{
    int stt = i + 1;
    <tr>
        <td width="5%">@stt</td>
        <td>@Model[i].ProductName</td>
        <td>@Model[i].Price</td>
        <td>@Model[i].Quantity</td>
        <td>@Model[i].TotalMoney</td>
    </tr>
}